  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "geocoding",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/index.ts",
        "${workspaceFolder}/test/sample-data.txt",
        "${workspaceFolder}/test/sample-output.txt",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "download command",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/index.ts",
        "download",
        "-f"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },

    {
      "type": "node",
      "request": "launch",
      "name": "update-check",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/index.ts",
        "update-check"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    }
  ]
}