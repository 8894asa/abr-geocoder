    {
      "type": "node",
      "request": "launch",
      "name": "temporally",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/cli/domain/AbrgMessage/__tests__/test.ts",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Jest Tests",
      "runtimeArgs": [
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand",
        "--coverage=false",
        "--watch",
        "--no-cache",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "geocoding",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/cli/interface-adapters/index.ts",
        "${workspaceFolder}/test/sample-data.txt",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "download command",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/cli/interface-adapters/index.ts",
        "download"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "update-check",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/cli/interface-adapters/index.ts",
        "update-check"
      ]
    }
  ]
}